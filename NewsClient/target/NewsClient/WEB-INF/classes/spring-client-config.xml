<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <import resource="classpath*:spring-config.xml"></import>

    <bean id="emptyCommand" class="com.epam.newsmanagement.command.EmptyCommand" autowire="byName"></bean>
    <bean id="filterCommand" class="com.epam.newsmanagement.command.EmptyCommand" autowire="byName"></bean>
    <bean id="resetFilterCommand" class="com.epam.newsmanagement.command.EmptyCommand" autowire="byName"></bean>
    <bean id="setLocaleCommand" class="com.epam.newsmanagement.command.SetLocaleCommand" autowire="byName"></bean>
    <bean id="sort_by_num_of_commentsCommand" class="com.epam.newsmanagement.command.EmptyCommand" autowire="byName"></bean>
    <bean id="sort_by_dateCommand" class="com.epam.newsmanagement.command.EmptyCommand" autowire="byName"></bean>
    <bean id="articlePageCommand" class="com.epam.newsmanagement.command.ArticlePageCommand" autowire="byName"/>
    <bean id="addCommentCommand" class="com.epam.newsmanagement.command.AddCommentCommand" autowire="byName"/>
    <bean id="loginCommand" class="com.epam.newsmanagement.command.LoginCommand" autowire="byName"/>
    <bean id="registryCommand" class="com.epam.newsmanagement.command.RegistryCommand" autowire="byName"/>
    <bean id="logoutCommand" class="com.epam.newsmanagement.command.LogoutCommand" autowire="byName"/>
</beans>